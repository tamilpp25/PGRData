    {
        "Id": 42,
        "ActionId": 42,
        "NextActionId": null,
        "Type": 301,
        "IsBlock": null,
        "IsEnd": null,
        "BeginAnim": null,
        "EndAnim": null,
        "BeginDelay": null,
        "EndDelay": null,
        "Params": [
            "To run into someone who has grown up as similar as you have, but ends up somewhere so drastically different from where you are."
