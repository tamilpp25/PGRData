        ]
    },
    {
        "Id": 16,
        "ActionId": 15,
        "NextActionId": null,
        "Type": 301,
        "IsBlock": null,
        "IsEnd": null,
        "BeginAnim": null,
        "EndAnim": null,
        "BeginDelay": null,
        "EndDelay": null,
        "Params": [
            "Snow",
