        "IsEnd": null,
        "BeginAnim": null,
        "EndAnim": null,
        "BeginDelay": null,
        "EndDelay": null,
        "Params": [
            "Huainan",
            "Which is funny, by the way. You won't believe how easy it is to sway the public's opinions.",
            2.0
        ]
    },
    {
        "Id": 25,
        "ActionId": 23,
        "NextActionId": null,
        "Type": 301,
        "IsBlock": null,
        "IsEnd": null,
        "BeginAnim": null,
        "EndAnim": null,
        "BeginDelay": null,
        "EndDelay": null,
        "Params": [
            "Huainan",
