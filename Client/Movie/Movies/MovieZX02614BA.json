        ]
    },
    {
        "Id": 8,
        "ActionId": 8,
        "NextActionId": null,
        "Type": 301,
        "IsBlock": null,
        "IsEnd": null,
        "BeginAnim": null,
        "EndAnim": null,
        "BeginDelay": null,
        "EndDelay": null,
        "Params": [
            "A prediction by humankind produces contradiction\u2014because, try as they may, humankind will run into unpredictable causality and time."
        ]
