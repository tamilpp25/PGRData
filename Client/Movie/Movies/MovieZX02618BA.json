        "IsBlock": null,
        "IsEnd": null,
        "BeginAnim": null,
        "EndAnim": null,
        "BeginDelay": null,
        "EndDelay": null,
        "Params": [
            "She just curls up in the vacuum, letting the abyss consume her."
        ]
    },
