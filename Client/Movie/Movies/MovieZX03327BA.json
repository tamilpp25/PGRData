        "IsBlock": null,
        "IsEnd": null,
        "BeginAnim": null,
        "EndAnim": null,
        "BeginDelay": null,
        "EndDelay": null,
        "Params": [
            "Seeing Ayla's hesitation, Meka can't help but begin to frown."
        ]
    },
    {
        "Id": 37,
        "ActionId": 37,
        "NextActionId": null,
        "Type": 204,
        "IsBlock": null,
        "IsEnd": null,
        "BeginAnim": null,
