    {
        "Id": 45,
        "ActionId": 45,
        "NextActionId": null,
        "Type": 301,
        "IsBlock": null,
        "IsEnd": null,
        "BeginAnim": null,
        "EndAnim": null,
        "BeginDelay": null,
        "EndDelay": null,
        "Params": [
            "Meka's hand was patting Ayla's head, but it suddenly flicks her on the forehead, catching her completely off-guard."
        ]
    },
    {
