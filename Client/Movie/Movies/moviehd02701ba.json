        "BeginAnim": null,
        "EndAnim": null,
        "BeginDelay": null,
        "EndDelay": null,
        "Params": [
            "Leonie",
            "Yup, just wait until the official version is released!",
            "1"
        ]
    },
    {
        "Id": 56,
        "ActionId": 56,
        "NextActionId": null,
        "Type": 301,
