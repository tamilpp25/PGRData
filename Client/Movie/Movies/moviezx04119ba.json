    {
        "Id": 34,
        "ActionId": 34,
        "NextActionId": null,
        "Type": 301,
        "IsBlock": null,
        "IsEnd": null,
        "BeginAnim": null,
        "EndAnim": null,
        "BeginDelay": null,
        "EndDelay": null,
        "Params": [
            "Forsaken Rookie",
